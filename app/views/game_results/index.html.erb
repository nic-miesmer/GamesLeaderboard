<h1>Listing Game Results</h1>

<table>
  <thead>
    <tr>
      <th>Player</th>
      <th>Game</th>
      <th>Date</th>
      <th>Win</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @game_results.each do |game_result| %>
      <tr>
        <td><%= game_result.user_id %></td>
        <td><%= game_result.game_id %></td>
        <td><%= game_result.date %></td>
        <td><%= game_result.win %></td>
        <td><%= link_to 'Show', game_result %></td>
        <td><%= link_to 'Edit', edit_game_result_path(game_result) %></td>
        <td><%= link_to 'Destroy', game_result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Game result', new_game_result_path %>
